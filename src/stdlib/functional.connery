; fun
(def {fun} (\ {f b} {
    def (head f) (\ (tail f) b)
}))

; flipargs
(fun {flipargs f a b} {f b a})

(fun {case & cs} {
   if (== cs None)
   {None}
   {if (first (first cs))
        {second (first cs)}
        {unpack case (tail cs)}}
})

; Currying
(fun {unpack f l} {
    eval (join (list f) l)
})
(fun {pack f & xs} {f xs})
(def {curry} unpack)
(def {uncurry} pack)